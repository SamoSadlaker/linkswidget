{
    "name": "samosadlaker/linkswidget",
    "description": "Just a simple widget to display links inside statamic CP.",
    "homepage": "https://github.com/SamoSadlaker/linkswidget",
    "keywords": ["statamic", "widget", "links"],
    "license": "MIT",
    "authors": [
        {
            "name": "SamoSadlaker",
            "email": "dev@samosadlaker.eu",
            "homepage": "https://samosadlaker.eu"
        }
    ],
    "autoload": {
        "psr-4": {
            "SamoSadlaker\\Linkswidget\\": "src"
        }
    },
    "require": {
        "statamic/cms": "^4.0 || ^5.0"
    },
    "extra": {
        "statamic": {
            "name": "Linkswidget",
            "description": "Just a simple widget to display links inside statamic CP."
        },
        "laravel": {
            "providers": [
                "SamoSadlaker\\Linkswidget\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
